{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 3,
    "stake_currency": "USDT",
    "stake_amount": 50,
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "EUR",
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "spot",
    "margin_mode": "",
    "bot_name": "signal-trader",
    "timeframe": "1h",
    "timeframe_detail": "",
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "kraken",
        "key": "",
        "secret": "",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "BTC/USDT",
            "ETH/USDT",
            "SOL/USDT",
            "XRP/USDT",
            "ADA/USDT",
            "AVAX/USDT",
            "DOT/USDT",
            "MATIC/USDT",
            "LINK/USDT",
            "ATOM/USDT",
            "LTC/USDT",
            "UNI/USDT",
            "AAVE/USDT",
            "NEAR/USDT",
            "ICP/USDT",
            "FIL/USDT",
            "ARB/USDT",
            "OP/USDT",
            "SUI/USDT",
            "APT/USDT",
            "ALGO/USDT",
            "XLM/USDT",
            "GRT/USDT",
            "INJ/USDT",
            "RUNE/USDT",
            "CRV/USDT",
            "MKR/USDT",
            "COMP/USDT",
            "LDO/USDT",
            "SNX/USDT",
            "DOGE/USDT",
            "ETC/USDT",
            "BCH/USDT",
            "TRX/USDT",
            "XTZ/USDT",
            "MANA/USDT",
            "SAND/USDT",
            "ENJ/USDT",
            "FLOW/USDT",
            "KSM/USDT",
            "ZEC/USDT",
            "DASH/USDT",
            "APE/USDT",
            "ENS/USDT",
            "IMX/USDT",
            "1INCH/USDT",
            "CHZ/USDT",
            "FTM/USDT",
            "YFI/USDT",
            "EOS/USDT"
        ],
        "pair_blacklist": [
            "BNB/.*"
        ]
    },
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],
    "stoploss": -0.05,
    "trailing_stop": false,
    "minimal_roi": {
        "120": 0.01,
        "60": 0.02,
        "30": 0.03,
        "0": 0.05
    },
    "order_types": {
        "entry": "limit",
        "exit": "limit",
        "stoploss": "market",
        "stoploss_on_exchange": false
    },
    "order_time_in_force": {
        "entry": "GTC",
        "exit": "GTC"
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "change-this-in-production-use-random-string",
        "ws_token": "change-this-in-production",
        "CORS_origins": [],
        "username": "trader",
        "password": "change-this-password"
    },
    "telegram": {
        "enabled": false,
        "token": "",
        "chat_id": ""
    },
    "internals": {
        "process_throttle_secs": 5
    }
}
